# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '8.0'
use_frameworks!


# Define main pods.
def main_pods
  #Your common pods here
  pod 'Alamofire', '~> 4.7'
  pod 'SwiftyJSON', '~> 4.0'
end

target 'AcrosureSDK' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  main_pods
  # Pods for AcrosureSDK

  target 'AcrosureSDKTests' do

    main_pods
    # Pods for testing
  end

end
